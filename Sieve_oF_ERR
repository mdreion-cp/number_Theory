#include <bits/stdc++.h>
using namespace std;
#define nl '\n'
#define ll long long
const double pi = 3.141592653589793;
const int MOD = 1e9 + 7;
const int sz = 100000 + 123;

void solve() {
	/*
	    Sieve of Eratosthenes
	    -> Generate all prime numbers up to n
	*/

	int n;
	cin >> n;

	vector<bool> isPrime(n + 1, true);
	isPrime[0] = isPrime[1] = false;

	for (int i = 2; i * i <= n; i++) {
		if (isPrime[i]) {
			for (int j = i * i; j <= n; j += i) {
				isPrime[j] = false;
			}
		}
	}

	vector<int> primes;
	for (int i = 2; i <= n; i++) {
		if (isPrime[i]) primes.push_back(i);
	}

	cout << "Primes up to " << n << ": ";
	for (auto p : primes) cout << p << " ";
	cout << nl;
}

int main() {
	ios::sync_with_stdio(false);
	cin.tie(nullptr);

	int t = 1;
	cin >> t;
	while (t--) {
		solve();
	}
	return 0;
}
